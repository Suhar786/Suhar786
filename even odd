def count_odd_even_numbers(numbers):
    odd_count = 0
    even_count = 0

    for number in numbers:
        if number % 2 == 0:
            even_count += 1
        else:
            odd_count += 1

    return odd_count, even_count

# Example usage
numbers_list = [2, 3, 4, 55, 56, 78, 75, 69, 66, 101, 100]
odd, even = count_odd_even_numbers(numbers_list)
print("Odd numbers count:", odd)
print("Even numbers count:", even)
